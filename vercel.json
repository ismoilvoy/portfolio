{
  "version": 2,
  "builds": [
    {
      "src": "api/market.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "img/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/market",
      "dest": "/api/market.js"
    },
    {
      "src": "/img/(.*)",
      "dest": "/img/$1"
    }
  ]
}